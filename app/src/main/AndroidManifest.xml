<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proctorialsystem">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".components.Reports.ShowRemarkList"></activity>
        <activity android:name=".components.Reports.NewProctorMeetActivity" />
        <activity android:name=".components.Reports.ReportDetailsActivity" />
        <activity android:name=".components.Dashboard.StudentDetails" />
        <activity
            android:name=".login.ProctorLoginActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/LoginTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ProctorMainActivity" />
    </application>

</manifest>